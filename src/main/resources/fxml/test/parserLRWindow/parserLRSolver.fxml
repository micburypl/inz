      <RowConstraints vgrow="SOMETIMES" />
      <RowConstraints percentHeight="1.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <ListView fx:id="parserLRInputList" prefWidth="343.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <VBox fx:id="parserLRButtonVBox" GridPane.columnIndex="1" GridPane.rowIndex="10">
          <children>
             <Button fx:id="printFirstButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#printFirst" styleClass="button-green" text="%parserLR.FirstButton" />
             <Button fx:id="printFollowButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#printFollow" styleClass="button-green" text="%parserLR.FollowButton" />
             <Button fx:id="printActionTableButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#printActionTable" styleClass="button-green" text="%parserLR.ActionTable" />
              <Button fx:id="printGotoTableButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#printGotoTable" styleClass="button-green" text="%parserLR.GotoTable" />

          </children>
      </VBox>
      <Label text="%parserLR.Solution" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
      <StackPane fx:id="parserLROutputPane" alignment="TOP_CENTER" GridPane.columnIndex="3" GridPane.rowIndex="4" GridPane.rowSpan="12" />
